def multiply (num1, num2):
    return num1 * num2
multiply (7,3)
print(multiply(13,75))

def multiply (num1, num2, num3, num4):
    return num1 * num2 + num3 + num4   #return is basically pressing equals

print(multiply(13,75,60,32))


pin = 1234
balance = 1000
account_number = 12345678
def print_dispensing_cash(pin_number, amount):
    global pin
    global balance
    global acc_entered
    if account_number == acc_entered:
        if pin_number == pin and amount <= balance:
            balance -= amount
            print(f"Pin correct, now dispensing £{amount}. Your remaining balance is balance £{balance}.")
        elif pin == pin_number and amount > balance:
            print(f"Not enough moneeehhhhhhh. Available is {balance}")
        elif pin != pin_number:
            print("Pin incorrect. You have three more tries. Let's roll those dice.")
    else:
        print("Check the ACC number!")
        start_atm()
def start_atm():
    global acc_entered
    acc_entered = int(input("What is the 8 digit account number?"))
    pin_number = int(input("What's your fackin PIN!"))
    amount_num = int(input("How much wonga?"))
    print_dispening_cash (pin_number, amount_num)
    start_atm()
    
    
    
    
